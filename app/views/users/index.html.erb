<% provide(:title, 'All users') %>
<h1 id = "all_users">All users</h1>

<%= form_tag users_path, :method => 'get' do %>
  <%= text_field_tag :search, params[:search], class: "search_box" %>  
  <%= submit_tag "Search", :name => nil  %>
<% end %>

<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>